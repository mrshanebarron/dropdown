{
    "name": "mrshanebarron/dropdown",
    "description": "Dropdown menu component for Laravel - supports Livewire and Vue",
    "keywords": ["laravel", "livewire", "vue", "dropdown", "menu", "popover"],
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MrShaneBarron\\Dropdown\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["MrShaneBarron\\Dropdown\\DropdownServiceProvider"]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
